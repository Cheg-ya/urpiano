{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "test": "jest --watch",
    "test:debug": "node --inspect ./node_modules/jest/bin/jest.js --runInBand --watch",
    "build": "webpack --mode production",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --inspect src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "peerjs": "^0.3.24",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.5.2",
    "react-icons": "^3.5.0",
    "react-loader-spinner": "^2.3.0",
    "react-piano": "^3.1.2",
    "react-router-dom": "^4.4.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "soundfont-player": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.17.3",
    "react-test-renderer": "^16.8.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/client/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less||scss)$": "identity-obj-proxy"
    }
  }
}
